  "description": "A Github webhook handler for transitioning Jira Issues",
  "license": "MIT",
  "require" : {
    "php": "^7.1",
    "monolog/monolog": "^1.22",
    "symfony/twig-bridge": "^3",
    "lesstif/php-jira-rest-client": "^1.10.7",
    "silex/silex": "^2.2",
    "php-http/guzzle6-adapter": "^1.1",
    "knplabs/github-api": "^2.6"
  },
  "require-dev": {
    "nocworx/phpcs": "dev-master as 1.0",
    "heroku/heroku-buildpack-php": "*",
    "phan/phan": "^0.10.0"
  },
  "repositories": [
    { "type": "vcs", "url": "https://github.com/nocworx/phpcs" }
  ],
  "autoload": {
    "psr-4": {
